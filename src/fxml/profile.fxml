      <Label fx:id="help" layoutX="276.0" layoutY="333.0" onMouseClicked="#help_popup" prefHeight="26.0" prefWidth="45.0" text="Help" textFill="#0026ff" underline="true">
         <font>
            <Font name="System Bold Italic" size="17.0" />
         </font>
      </Label>
      <Button layoutX="373.0" layoutY="335.0" mnemonicParsing="false" onAction="#change_password" text="CHANGE" />
