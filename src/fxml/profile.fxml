            <Font name="System Bold" size="20.0" />
         </font>
      </Label>
      <PasswordField fx:id="old_password" alignment="CENTER" layoutX="214.0" layoutY="221.0" promptText="Old Password">
         <font>
            <Font name="System Bold" size="13.0" />
         </font></PasswordField>
      <PasswordField fx:id="new_password" alignment="CENTER" layoutX="214.0" layoutY="264.0" promptText="New Pasword">
         <font>
            <Font name="System Bold" size="13.0" />
         </font></PasswordField>
      <OctIconView fill="#ffffcc" glyphName="HOME" layoutX="26.0" layoutY="45.0" onMouseClicked="#return_home" scaleX="3.0" scaleY="3.0" strokeType="OUTSIDE" strokeWidth="0.0" />
      <Label alignment="CENTER" layoutX="18.0" layoutY="54.0" prefHeight="7.0" prefWidth="28.0" text="HOME" textFill="#ffffcc">
