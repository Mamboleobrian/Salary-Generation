<AnchorPane fx:id="anchor_profile" prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #ffffcc;" stylesheets="@../styles/ueps.css" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.profile">
   <children>
