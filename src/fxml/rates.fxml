<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="anchor_rates" prefHeight="400.0" prefWidth="1000.0" stylesheets="@../styles/ueps.css" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.rates">
   <children>
      <Label alignment="CENTER" layoutX="10.0" layoutY="14.0" prefHeight="47.0" style="-fx-background-color: #5d1f00;" text="PAYMENT RATES" textFill="#ffffcc" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0">
         <font>
            <Font name="System Bold" size="20.0" />
         </font></Label>
      <HBox alignment="CENTER" layoutX="1.0" layoutY="70.0" prefHeight="317.0" spacing="10.0" AnchorPane.leftAnchor="20.0" AnchorPane.rightAnchor="20.0">
         <children>
            <VBox alignment="CENTER" prefHeight="100.0" prefWidth="540.0" spacing="20.0">
               <children>
                  <Label alignment="CENTER" prefHeight="30.0" prefWidth="225.0" text="CURRENT RATES" textFill="#19355e">
                     <font>
                        <Font name="System Bold" size="18.0" />
                     </font>
                  </Label>
                  <TableView fx:id="tableview_rate" prefHeight="200.0" prefWidth="437.0">
                    <columns>
                      <TableColumn fx:id="jurisdiction" prefWidth="223.0" text="JURISDICTION" />
                      <TableColumn fx:id="p_rates" prefWidth="152.0" text="RATE PER HOUR" />
                        <TableColumn fx:id="ovr_rates" prefWidth="165.0" text="OVERTIME RATE PER HOUR" />
                    </columns>
                     <columnResizePolicy>
                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                     </columnResizePolicy>
                  </TableView>
               </children>
            </VBox>
            <Separator orientation="VERTICAL" prefHeight="200.0" />
            <VBox alignment="CENTER" prefHeight="317.0" prefWidth="521.0" spacing="40.0">
               <children>
                  <Label alignment="CENTER" prefHeight="30.0" prefWidth="450.0" text="CHANGE RATES" textFill="#19355e">
                     <font>
                        <Font name="System Bold" size="18.0" />
                     </font>
                  </Label>
                  <HBox alignment="TOP_CENTER" prefHeight="41.0" prefWidth="452.0" spacing="10.0">
                     <children>
                        <ComboBox editable="true" prefWidth="150.0" promptText="Jurisdiction" stylesheets="@../styles/ueps.css" />
                        <TextField promptText="New Rate Per Hour">
                           <font>
                              <Font name="System Bold" size="12.0" />
                           </font>
                        </TextField>
                        <TextField promptText="New Overtime Rate Per Hour">
                           <font>
                              <Font name="System Bold" size="12.0" />
                           </font>
                        </TextField>
                     </children>
                  </HBox>
                  <Button mnemonicParsing="false" text="CHANGE" />
                  <Label prefHeight="26.0" prefWidth="43.0" text="Help" textFill="#0026ff" underline="true">
                     <font>
                        <Font name="System Bold Italic" size="17.0" />
                     </font>
                  </Label>
               </children>
            </VBox>
         </children>
      </HBox>
   </children>
</AnchorPane>
